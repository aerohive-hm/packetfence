#!/usr/bin/perl
=head1 NAME

pftest.pl

=cut

=head1 DESCRIPTION

The driver script for pftest

=cut

use strict;
use warnings;
use FindBin qw($Bin);
use lib "$Bin/../lib";

use pf::pftest;
exit pf::pftest->new({args => \@ARGV})->run();

