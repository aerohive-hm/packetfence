                # Note: the grid has 18 columns.
                #
                IF cluster.keys.size == 1;
                  SET spanSize = gridSize / 2;
                ELSIF cluster.keys.size MOD 2 == 0;
                  SET spanSize = gridSize / 2;
                ELSE;
                  SET spanSize = gridSize / 3;
                END
             %]
