#!/bin/sh
update_log_file=/usr/local/pf/logs/a3_cluster_update.log
echo "restart a3 api backend in post process" >> $update_log_file
systemctl stop a3-api-backend
systemctl start a3-ama
cd /usr/local/pf/a3_update
/usr/bin/node restart_nodeapp_wrapper.js
echo "finish actions in post process" >> $update_log_file
